{
  "name": "@kedao/finder",
  "version": "0.0.2",
  "description": "Media manager for Kedao Editor.",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/banyudu/kedao.git"
  },
  "scripts": {
    "copyfiles": "copyfiles -u 1 src/**/*.{scss,woff,ttf,json} lib/",
    "build:lib": "rm -rf lib/ && tsc && npm run copyfiles",
    "build:watch": "tsc -w",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "kedao",
    "editor",
    "finder",
    "media-manager"
  ],
  "author": "banyudu",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1"
  }
}
