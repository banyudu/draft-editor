{
  "name": "kedao",
  "version": "0.0.2",
  "description": "Rich Text Editor Based On Draft.js",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "banyudu",
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint --fix . --ext .js,.jsx,.ts,.tsx",
    "prepublishOnly": "npm run build",
    "copyfiles": "copyfiles -u 1 'src/**/*.{scss,woff,ttf,json}' lib/",
    "build": "rm -rf lib && tsc && npm run copyfiles",
    "build:watch": "tsc -w"
  },
  "repository": "https://github.com/banyudu/kedao",
  "keywords": [
    "kedao",
    "draft",
    "draft-js",
    "rich",
    "text",
    "editor",
    "rich-text-editor",
    "wysiwyg"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/draft-js": "^0.11.8",
    "@types/node": "^17.0.13",
    "@types/react": "^16.9.23",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "copyfiles": "^2.4.1",
    "draft-js-prism": "^1.0.6",
    "eslint": "^8.8.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "lint-staged": "^12.3.2",
    "prettier": "^2.5.1",
    "prism-js": "^1.0.0",
    "prismjs": "^1.15.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@draft-js-plugins/mention": "^5.1.1",
    "draft-convert": "^2.1.8",
    "draft-js": "^0.11.5",
    "draft-js-multidecorators": "^1.0.0",
    "draftjs-utils": "^0.10.2",
    "immutable": "~4.0.0",
    "merge-class-names": "^1.4.2",
    "prop-types": "^15.7.2",
    "react-color": "^2.14.1",
    "react-icons": "^4.3.1",
    "uuid": "^8.3.2"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
