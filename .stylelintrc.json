{
  "extends": "stylelint-config-standard-scss",
  "rules": {
    "scss/at-import-partial-extension": "always",
    "no-descending-specificity": null,
    "font-family-no-missing-generic-family-keyword": [
      true,
      {
        "ignoreFontFamilies": "bf-icons"
      }
    ]
  },
  "plugins": [
    "stylelint-scss"
  ]
}
